{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules":{
        "linebreak-style": [
            "error",
            "windows"
        ],
        "import/extensions": 0,
        "array-callback-return": 0,
        "import/no-dynamic-require":0,
        "global-require":0
    }
}
