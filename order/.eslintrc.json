{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules":{
        "linebreak-style": [
            "error",
            "unix"
        ],
        "import/extensions": 0,
        "array-callback-return": 0,
        "new-cap":0,
        "no-shadow":0,
        "no-console":0
    }
}
