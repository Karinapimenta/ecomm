{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": 0,
        "linebreak-style": [
            "error",
            "windows"
        ],
        "array-callback-return": 0,
        "no-shadow":0,
        "consistent-return":0
    }
}