db.products.aggregate([{
    $project:{
        "_id":"$_id",
        "product":"$product",
        "description":"$description",
        "slug":"$slug",
        "pricePerUnit":{$round:[{$toDecimal:"$pricePerUnit"},2]},
        "quantityInStock":"$quantityInStock",
        "category":"$category",
    }
},{
    $out:"products"
}
])